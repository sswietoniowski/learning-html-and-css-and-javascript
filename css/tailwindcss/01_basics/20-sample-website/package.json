{
  "name": "starting",
  "version": "1.0.0",
  "description": "Sample Tailwind",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev-*",
    "dev-js": "rollup --config -w",
    "dev-css": "npx tailwindcss -i ./src/app.src.css -o ./public/css/app.css --watch",
    "build": "run-p build-*",
    "build-js": "rollup --config rollup.config.prod.js",
    "build-css": "npx tailwindcss -i ./src/site.scss -o ./public/css/app.css --minify"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "feather-icons": "^4.29.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.29.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-sass": "^1.12.20",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.3.3"
  }
}
